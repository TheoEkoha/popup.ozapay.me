}
.progressFirstStep,
.progressSecondStep,
.progressThirdStep,
.progressFourStep,
.progressFifthStep {
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
  background: #17bfcc;
}
.progressFirstStep {
  width: 10%;
}
.progressSecondStep {
  width: 30%;
}
.progressThirdStep {
  width: 50%;
}
.progressFourStep {
  width: 75%;
}
.progressFifthStep {
  width: 100%;
}

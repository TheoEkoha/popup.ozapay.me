  .buttonRegister input[type="button"] {
    margin-right: 0 !important;
    margin-bottom: 0rem;
    margin-top: 2rem;

  }
  .modalLeft .arrow {
    position: absolute !important;
    margin-bottom: 0;
    width: 1rem;
    bottom: 2.95rem;
    left: 20rem;
    top: 1rem;
  }

  .modalLeft input {
    width: 22rem!important;
    font-size: 1rem!important;
    margin-right: 0!important;
    padding: 10px 15px;
    margin-bottom: 0rem;
    

    
  }
  .logoContainer{
	margin-bottom:0!important;  
  }
  .welcome h2{
	font-size: 22px!important;  
  }
  .welcome p{
      margin-bottom: 2rem!important;
  }
  .modalRightShadow {
	
    
	 top: 31%;
	left: 50%!important;
	padding: 3rem 2rem;
  }
  .modalRightShadow img{
	width:12rem!important;  
  }
  .bottomShadow{
	margin-top:0!important;
  }
  .modalContainer img{
	margin-bottom:0!important;
  }
}
.modalRightRegister {
  display: flex;
  justify-content: flex-end;
  width: 50%;
  height: 100%;
  background: url("/openaccount.jpg") center;
  background-size: cover;
  color: #e0e0e0;
  text-shadow: 1px 1px 4px #000 !important;
}

.closeModal {
  position: absolute;
  font-size: 1.875rem;
  color: #858585;
  
}
.closeModalDesktop{
	display:block;
    right: 2rem;
    top: 1.4rem;
}
.closeModalMobile{
    display:none;	
}
.closeModal:hover {
  cursor: pointer;
}

.lostPasswordButton {
  color: #01b9c7 !important;
  display: flex;
  padding-top: 1rem;
}

.lostPasswordButton:hover {
  cursor: pointer;
}

.hideForm {
  display: none !important;
}

.registerMessage {
  margin-top: 2rem;
  color: #01b9c7 !important;
  font-weight: 600;
}

.hideRegisterMessage {
  display: none;
}

.modalPasswordError {
  background-color: #dd5680;
  width: 30%;
  height: auto;
  position: absolute;
  top: calc(20% * 1.2);
  right: calc(16% * 1.2);
  border: rgba(255, 0, 0, 0.5) solid 1px;
  border-radius: 30px;
  color: white !important;
  padding: 1em;
  font-size: 1rem;
}

@media only screen and (max-width: 1024px) {
  .modalRightRegister {
    background: none;
    width: 5% !important;
    display: none;
    color: #e0e0e0;
  }
  .modalRightRegister svg {
    text-shadow: 1px 1px 4px #000 !important;
  }
  .registerButton {
    display: block;
  }
  
 
}
@media only screen and (max-width: 1024px) {
  
  .modalRightRegister svg {
    text-shadow: 1px 1px 4px #000 !important;
  }
  
}
